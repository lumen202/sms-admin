<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import atlantafx.base.controls.ModalPane?>

<StackPane xmlns="http://javafx.com/javafx/23.0.1" 
           xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="finalproject.admin.app.home.HomeController">
    <children>
        <!-- Container used for swapping in new scenes -->
        <StackPane fx:id="contentPane">
            <children>
                <BorderPane prefHeight="400.0" prefWidth="600.0">
                    <center>
                        <TableView fx:id="clusterTableView" prefHeight="400.0" prefWidth="600.0">
                            <columns>
                                <TableColumn fx:id="clusterID" prefWidth="75.0" text="ClusterID"/>
                                <TableColumn fx:id="clusterName" prefWidth="75.0" text="ClusterName"/>
                            </columns>
                        </TableView>
                    </center>
                </BorderPane>
                <Button fx:id="testButton" text="Open Student Form"/>
            </children>
        </StackPane>
        <!-- Declare ModalPane without any children -->
        <ModalPane fx:id="formodal"/>
    </children>
</StackPane>
